@model HairSalon.Models.Stylist

@{
    ViewData["Title"] = "Stylist Details";
}

<h2>@ViewData["Title"]</h2>

<div>
    <dl class="dl-horizontal">
        <dt>Name</dt>
        <dd>@Model.Name</dd>

        <dt>Specialty</dt>
        <dd>@Model.Specialty</dd>
    </dl>
</div>

<div>
    <a asp-action="Edit" asp-route-id="@Model.StylistId">Edit</a> |
    <a asp-action="Delete" asp-route-id="@Model.StylistId">Delete</a> |
    <a asp-action="Index">Back to List</a>
</div>